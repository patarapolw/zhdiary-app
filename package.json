{
  "name": "zhdiary-app",
  "version": "0.4.0",
  "description": "Zhdiary flashcard and diary app",
  "main": "dist/electron.min.js",
  "repository": "https://zhlevel.herokuapp.com",
  "author": "Pacharapol Withayasakpunt",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@types/dotenv": "^6.1.1",
    "@types/hyperscript": "^0.0.3",
    "@types/jquery": "^3.3.29",
    "@types/showdown": "^1.9.3",
    "@types/simplemde": "^1.11.7",
    "@types/toastr": "^2.1.37",
    "@types/wait-on": "^3.2.0",
    "@types/xregexp": "^3.0.30",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.23",
    "css-loader": "^3.0.0",
    "dotenv": "^8.0.0",
    "electron": "^5.0.4",
    "electron-builder": "^20.43.0",
    "expose-loader": "^0.7.5",
    "file-loader": "^4.0.0",
    "flatpickr": "^4.6.1",
    "hyperscript": "^2.0.2",
    "jquery": "^3.4.1",
    "jquery-contextmenu": "^2.8.0",
    "node-sass": "^4.12.0",
    "popper.js": "^1.15.0",
    "raw-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "showdown": "^1.9.0",
    "style-loader": "^0.23.1",
    "sweetalert": "^2.1.2",
    "toastr": "^2.1.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6",
    "vue-simplemde": "^0.5.2",
    "vue-template-compiler": "^2.6.10",
    "wait-on": "^3.2.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.7.1",
    "xregexp": "^4.2.4"
  },
  "build": {
    "appId": "io.github.patarapolw.rep2recall",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.education",
      "icon": "icon.png"
    },
    "files": [
      "asset/**/*",
      "public/**/*",
      "dist/pyserver",
      "dist/electron.min.js",
      "package.json"
    ],
    "asarUnpack": [
      "asset/**/*",
      "public/**/*",
      "dist/pyserver"
    ]
  },
  "scripts": {
    "start": "yarn run dev",
    "dev": "concurrently 'yarn run js:dev' 'yarn run py:dev'",
    "js:dev": "rimraf public/js && webpack-dev-server --config webpack.dev.js",
    "py:dev": "poetry run python pyserver.py",
    "build": "yarn run js:build && yarn run py:build",
    "js:build": "rimraf public/js dist && webpack --config webpack.prod.js",
    "py:build": "poetry run pyinstaller -wF pyserver.py",
    "electron:start": "electron .",
    "electron:dev": "yarn run py:build && webpack --config webpack.prod.js --mode=development -w",
    "electron:build": "rimraf release && yarn run build && electron-builder"
  }
}
